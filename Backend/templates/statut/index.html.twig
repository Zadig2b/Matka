{% extends 'base.html.twig' %}

{% block title %}Statut index{% endblock %}

{% block body %}
    <h1>Statut index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for Statut in Statuts %}
            <tr>
                <td>{{ Statut.id }}</td>
                <td>{{ Statut.nom }}</td>
                <td>
                    <a href="{{ path('app_Statut_show', {'id': Statut.id}) }}">show</a>
                    <a href="{{ path('app_Statut_edit', {'id': Statut.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_Statut_new') }}">Create new</a>
{% endblock %}
